{
    "kind": "Deployment",
    "apiVersion": "extensions/v1beta1",
    "metadata": {
        "name": "vegeta-broker",
        "creationTimestamp": null,
        "labels": {
            "app": "vegeta",
            "tier": "frontend"
        }
    },
    "spec": {
        "replicas": 2,
        "selector": {
            "matchLabels": {
                "app": "vegeta",
                "tier": "frontend"
            }
        },
        "template": {
            "metadata": {
                "creationTimestamp": null,
                "labels": {
                    "app": "vegeta",
                    "tier": "frontend"
                }
            },
            "spec": {
                "containers": [
                    {
                        "name": "vegeta-broker",
                        "image": "registry.hub.docker.com/souranilsbu/vegeta",
                        "ports": [
                            {
                                "containerPort": 1883
                            }
                        ],
                        "env": [
                            {
                                "name": "GET_HOSTS_FROM",
                                "value": "dns"
                            }
                        ],
                        "resources": {}
                    }
                ],
		"imagePullSecrets": [
		  {
		    "name": "regsecret"
		  }
		]
            }
        },
        "strategy": {}
    },
    "status": {}
}
