{
  "name": "vegeta",
  "description": "A MQTT Broker-Subscriber for IoT cluster",
  "version": "0.0.1",
  "contributors": [
    "Souranil Sen <souranil.sen@stonybrook.edu>",
    "Abhinandan Dubey <abhinandan.dubey@stonybrook.edu>"
  ],
  "keywords": [
    "mqtt",
    "publish/subscribe",
    "iot"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/sladebot/vegeta.git"
  },
  "main": "mqtt.js",
  "scripts": {
