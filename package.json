    "global": "^4.3.1",
    "gulp": "^3.9.1",
    "gulp-ignore": "^2.0.2",
    "mosca": "^2.3.0",
    "mqtt": "^2.4.0",
    "redis": "^2.7.1",
    "snazzy": "^6.0.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.0",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "gulp-babel": "^6.1.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "should": "*",
    "sinon": "~1.17.7",
    "standard": "^9.0.2",
    "uglify": "^0.1.5",
